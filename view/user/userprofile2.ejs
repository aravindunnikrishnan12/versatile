  <script>
    const form = document.getElementById('editProfileForm');
    const username = document.getElementById('newName');
    const nameError = document.getElementById('name_error');
    form.addEventListener('submit', (e) => {
              let isValid = true;
      
              // Clear previous error messages
              nameError.innerHTML = "";
             
              if (username.value.trim() === '' || username.value === null||!/^[a-zA-Z0-9]+$/.test(username.value)) {
                  e.preventDefault();
                  nameError.innerHTML = "Name is required and can only contain letters";
                  isValid = false;
              }
            });
      
  </script>



